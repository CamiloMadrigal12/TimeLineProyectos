{
  
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/consulta-radicado",
      "dest": "/api/consulta_radicado.js"
    },
    {
      "src": "/api/test-cors",
      "dest": "/api/test-cors.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
